#include "../definitions.zir"
#include "../genTemplateSubframe.zir"


fun comp test_genTemplateSubframe() {
var cellGrid : arr[NDLRB*12*14] complex16;

  let tx = configTX();

  let frame = 0 in 
  let subframe = 0 in 
  genTemplateSubframe(cellGrid, subframe, tx, true);
  emits cellGrid;

  let frame = 1 in 
  let subframe = 2 in 
  genTemplateSubframe(cellGrid, subframe, tx, true);
  emits cellGrid;

  let frame = 3 in 
  let subframe = 5 in 
  genTemplateSubframe(cellGrid, subframe, tx, true);
  emits cellGrid;

  let frame = 10 in 
  let subframe = 9 in 
  genTemplateSubframe(cellGrid, subframe, tx, true);
  emits cellGrid;

};

let comp main = read[int] >>> test_genTemplateSubframe() >>> write


